  "license": "MIT",
  "scripts": {
    "build": "nx run-many -t build",
    "test": "nx run-many -t test",
    "lint": "nx run-many -t lint",
    "serve": "nx run-many -t serve",
    "format": "nx format:write",
    "amazon-q:auto": "node docs/6_fixing/scripts/amazon-q-auto.js",
    "gemini:auto": "node docs/6_fixing/scripts/gemini-auto.js",
    "dashboard:monitor": "node docs/6_fixing/scripts/monitor-dashboard.js",
    "notifications:start": "node docs/6_fixing/scripts/notifications.js",
    "activate:auto-system": "docs/6_fixing/scripts/AUTO_SYSTEM_LAUNCHER.bat",
    "dashboard": "node docs/6_fixing/dashboard/server.js",
    "dashboard:standalone": "start http://localhost:3000 && node docs/6_fixing/dashboard/server.js",
    "repair:run": "node docs/6_fixing/scripts/nx_auto_fix.js",
    "auto-fix:start": "cd packages/tooling/auto-fix-system && npm start",
    "auto-fix:cycle": "cd packages/tooling/auto-fix-system && npm run cycle",
    "auto-fix:health": "cd packages/tooling/auto-fix-system && npm run health",
    "system:health": "node docs/6_fixing/scripts/test-system.bat",
    "unified:launcher": "docs/6_fixing/core/UNIFIED_LAUNCHER.bat",
    "firebase:emulators": "firebase emulators:start --only dataconnect",
    "firebase:deploy": "firebase deploy",
    "firebase:test": "firebase emulators:exec --only dataconnect \"npm test\"",
    "dataconnect:generate": "firebase dataconnect:sdk:generate --language=javascript",
    "dataconnect:migrate": "firebase dataconnect:sql:migrate",
    "dataconnect:diff": "firebase dataconnect:sql:diff",
    "dataconnect:dev": "firebase emulators:start --only dataconnect,ui",
    "db:migrate:test": "echo 'Test database migration - using mocks'",
    "test:quick": "vitest tests/unit/sidebar-agents.test.ts",
    "test:connections": "echo 'Testing connections - Firebase and Gemini'",
    "dev": "nx run-many -t serve --parallel=3",
    "dev:web-chatbot": "nx serve web-chatbot",
    "dev:admin-dashboard": "nx serve admin-dashboard",
    "dev:api": "nx serve api",
    "dev:all": "nx run-many -t serve --parallel=5",
    "clean": "nx reset && rm -rf node_modules",
    "security:audit": "npm audit && pnpm dlx @snyk/cli test",
    "security:check": "node -e \"console.log('üîí Security check: OK')\"",
    "env:check": "node -e \"if(!require('fs').existsSync('.env')){console.error('‚ùå .env file missing! Copy from .env.example');process.exit(1)}else{console.log('‚úÖ .env file found')}\"",
    "setup:dev": "npm run env:check && pnpm install && npm run test:quick"
  },
