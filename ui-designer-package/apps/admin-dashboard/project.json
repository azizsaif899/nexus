{
  "name": "admin-dashboard-designer",
  "sourceRoot": "apps/admin-dashboard/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/admin-dashboard-designer",
        "configFile": "ui-designer-package/apps/admin-dashboard/vite.config.ts"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "admin-dashboard:build",
        "port": 4200
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "coverage/apps/admin-dashboard"
      }
    }
  },
  "tags": ["type:app", "scope:admin"]
}