# متغيرات البيئة لنظام الإصلاح الذاتي

# مفتاح Gemini AI للإصلاح الذكي
GEMINI_API_KEY=your_gemini_api_key_here

# إعدادات النظام
AUTO_FIX_ENABLED=true
MAX_FIXES_PER_RUN=5
MIN_CONFIDENCE_THRESHOLD=0.7

# إعدادات الاختبار
RUN_TESTS_AFTER_FIX=true
BACKUP_FILES=true

# إعدادات التقارير
REPORTS_DIR=../docs/6_fixing/reports
SAVE_DETAILED_LOGS=true