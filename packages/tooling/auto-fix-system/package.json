    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "keywords": [
    "auto-fix",
    "deep-scan",
    "security",
    "typescript",
    "code-quality",
    "automation"
  ],
  "author": "AzizSys AI Assistant",
  "license": "MIT",
  "repository": {
